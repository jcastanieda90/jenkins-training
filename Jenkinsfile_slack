pipeline {
    agent any

    stages {
        stage('Slack Message') {
            steps {
                slackSend channel: '#jenkins-dsg',
                color: '#FFFF00',
                message: "*${currentBuild.currentResult}:* @here Deploy to prod muts be approved by @fcastaneda ${env.JOB_NAME} build ${env.BUILD_NUMBER}\n More info at: ${env.BUILD_URL}

            }
        }
    }
}
